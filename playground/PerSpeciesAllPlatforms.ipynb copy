    "def build_volume_df_all(databases):\n",
    "    volume_all = {}\n",
    "    for name, db in databases.items():\n",
    "        wild_counts_all_species = db.getWildCountsAllSpecies(name)\n",
    "        volume_all[name] = wild_counts_all_species\n",
    "    return volume_all"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "databases = {\"youtube\": yt_db, \"flickr\": fr_db, \"iNaturalist\": inat_db}\n",
    "\n",
    "volume_all = build_volume_df_all(databases)"
   ]
  },
